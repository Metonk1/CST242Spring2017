package p2;

import java.util.Arrays;

public class SortArrayDemo {

	public static void main(String[] args) {
		int[] a = new int[10];
		for(int i = 0; i < a.length; i++) {
			a[i] = (int)(Math.random() * 10); // random() is a static method
		}
		System.out.println(Arrays.toString(a)); // toString() is also a static method
		Arrays.sort(a);
		System.out.println(Arrays.toString(a));
		
		
	}

}
