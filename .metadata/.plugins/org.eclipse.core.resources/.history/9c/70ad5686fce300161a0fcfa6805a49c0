package p1;

public class Demo {

	public static void main(String[] args) {
		Student s1 = new Student();
		s1.setName("Adam");
		s1.setId("1111");
		s1.setGpa(3.0);
		
		System.out.println(s1.getName());
		System.out.println(s1.getId());
		System.out.println(s1.getGpa());
		
		Student s2 = s1;
		
		s2.setName("Eve");
		System.out.println(s1.getName());
		
	}

}
