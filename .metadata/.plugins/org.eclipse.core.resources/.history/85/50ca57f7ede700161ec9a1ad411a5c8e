package p1;

public class Instructor {
	private String name;
	private double salary;
	private String id;

	public Instructor(String name, double salary, String id) {
		super();
		this.name = name;
		this.salary = salary;
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getSalary() {
		return salary;
	}

	public void setSalary(double salary) {
		this.salary = salary;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	@Override
	public String toString() {
		return "Instructor [name=" + name + ", salary=" + salary + ", id=" + id + "]";
	}

}
